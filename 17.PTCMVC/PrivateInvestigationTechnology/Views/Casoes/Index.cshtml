@model IEnumerable<PrivateInvestigationTechnology.Models.Caso>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CadenaCustodia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InvestigacionExtorsion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstudiosSeguridad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InvestigacionInfidelidades)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InvestigacionRobosEmpresariales)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Antecedentes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RecuperacionVehiculos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdClienteNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdDetectiveNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CadenaCustodia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InvestigacionExtorsion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstudiosSeguridad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InvestigacionInfidelidades)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InvestigacionRobosEmpresariales)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Antecedentes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RecuperacionVehiculos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdClienteNavigation.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdDetectiveNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
